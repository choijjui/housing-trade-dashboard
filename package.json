{
  "name": "housing-trade-dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.2",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.19.15",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "babel-plugin-root-import": "^6.6.0",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-preset-react-app": "^10.0.0",
    "d3": "^6.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "styled-components": "^5.2.1",
    "typescript": "^4.1.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@types/d3": "^6.2.0"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "styled-components"
      ],
      [
        "babel-plugin-root-import",
        {
          "paths": [
            {
              "rootPathPrefix": "~",
              "rootPathSuffix": "./src"
            }
          ]
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "quotes": [
        "warn",
        "single",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ],
      "semi": [
        "warn",
        "always"
      ],
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1,
          "VariableDeclarator": 1,
          "flatTernaryExpressions": true,
          "ObjectExpression": "first",
          "ignoredNodes": [
            "TemplateLiteral"
          ]
        }
      ],
      "object-curly-spacing": [
        "warn",
        "always"
      ],
      "array-bracket-spacing": [
        "warn",
        "never"
      ],
      "arrow-parens": [
        "warn",
        "as-needed"
      ],
      "multiline-ternary": [
        "warn",
        "always-multiline"
      ],
      "strict": 0,
      "comma-dangle": [
        "warn",
        "only-multiline"
      ],
      "jsx-quotes": [
        "error",
        "prefer-double"
      ],
      "template-curly-spacing": "off",
      "react/prefer-stateless-function": 0,
      "react/jsx-filename-extension": 0,
      "react/jsx-one-expression-per-line": 0,
      "react-hooks/exhaustive-deps": 0
    }
  }
}
